package assignment_one;

public class main {

	public static void main(String[] args) {
		System.out.println("Called from main.");
	}

	public static void createQueue(int capacity) {
			// create queue object with capacity of 10
			QueueIm queue = new QueueIm(capacity);

			// add x items to queue
			for (int i = 0; i < capacity; i++) {
				queue.enqueue(i);
			}
			// remove all items from queue
			for (int i = 0; i < capacity; i++) {
				queue.dequeue();
			}
	}

	public static void createLinkedList() {
			LinkedListIm<Integer> ll = new LinkedListIm<Integer>();

			for (int i = 0; i < 10; i++) {
				ll.addAfter(null, i);
			}

			//ll.addAfter(76, 54);
			//ll.deleteAfter(76);
			ll.traverse(false);

			ll.addAfter(3, 100);
			ll.traverse(false);
	}

}
