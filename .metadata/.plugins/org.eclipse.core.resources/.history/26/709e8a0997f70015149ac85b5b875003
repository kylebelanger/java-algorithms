package assignment_two;

class Time {
  int hour, minute;
  double second;

  public Time () {
    this.hour = 0;
    this.minute = 0;
    this.second = 0.0;
  }

  public Time (int hour, int minute, double second) {
    this.hour = hour;
    this.minute = minute;
    this.second = second;
  }

  private printTime() {
    System.out.println("Hour: " + hour, + "Minute: " + minute + "Second: " + second);
  }
}
